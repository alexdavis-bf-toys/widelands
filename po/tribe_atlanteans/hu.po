msgid "Guardhall"
msgstr "Őrterem"

#: ../../tribes/atlanteans/conf:199
msgid "Guardhouse"
msgstr "Őrház"

#: ../../tribes/atlanteans/conf:76
msgid "Hammer"
msgstr "Kalapács"

#: ../../tribes/atlanteans/blackroot_farm/conf:24
msgid "Harvest blackroot"
msgstr "a feketegyökér aratása"

#: ../../tribes/atlanteans/farm/conf:26
