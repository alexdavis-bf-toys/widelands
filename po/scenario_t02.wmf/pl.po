#: ../../campaigns/t02.wmf/scripting/mission_thread_texts.lua:248
#: ../../campaigns/t02.wmf/scripting/mission_thread_texts.lua:274
#: ../../campaigns/t02.wmf/scripting/mission_thread_texts.lua:284
#: ../../campaigns/t02.wmf/scripting/mission_thread_texts.lua:299
#: ../../campaigns/t02.wmf/scripting/mission_thread_texts.lua:317
#: ../../campaigns/t02.wmf/scripting/mission_thread_texts.lua:338
#: ../../campaigns/t02.wmf/scripting/mission_thread_texts.lua:359
#: ../../campaigns/t02.wmf/scripting/mission_thread_texts.lua:368
msgid "Your loyal companion"
msgstr "Twój wierny towarzysz"
