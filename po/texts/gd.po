msgstr "F: Toglaich làn-sgrìn"

#: ../../txts/editor_readme:27
msgid "H: toggles main menu"
msgstr "H: Toglaich am prìomh chlàr-taice"

#: ../../txts/editor_readme:31
msgid "P: toggles player menu"
msgstr "P: Toglaich clàr-taice a' chluicheadair"

#: ../../txts/editor_readme:33
msgid "T: toggles tools menu"
msgstr "T: Toglaich clàr-taice nan inneal"

#: ../../txts/editor_readme:35
msgid "I: activates information tool"
msgstr "I: Gnìomhaich an t-inneal fiosrachaidh"

#: ../../txts/editor_readme:37
msgid "CTRL+L: load map"
msgstr "CTRL+L: luchdaich mapa"

#: ../../txts/editor_readme:39
msgid "CTRL+S: save map"
msgstr "CTRL+S: sàbhail mapa"

#: ../../txts/editor_readme:41
msgid "1-0: changes tool size"
msgstr "1-0: Atharraich meud nan innealan"

#: ../../txts/editor_readme:43
msgid "SHIFT (Hold): selects first alternative tool while pressed"
msgstr ""
"SHIFT (Cum sìos): Tagh an dàrna inneal dàrnach fhad 's a thèid a chumail sìos"

